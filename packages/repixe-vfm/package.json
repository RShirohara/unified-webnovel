{
  "name": "@rshirohara/repixe-vfm",
  "version": "0.1.0-alpha.1",
  "description": "repixe plugin that turns pixiv novel format into Vivlyostyle Flavored Markdown to support vfm.",
  "keywords": ["unified", "repixe", "pixiv", "markdown", "vfm", "vivliostyle"],
  "homepage": "https://github.com/RShirohara/unified-webnovel/tree/main/packages/repixe-vfm#readme",
  "bugs": {
    "url": "https://github.com/RShirohara/unified-webnovel/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RShirohara/unified-webnovel.git",
    "directory": "packages/repixe-rekurke"
  },
  "license": "MIT",
  "author": {
    "name": "Ray Shirohara",
    "url": "https://github.com/RShirohara"
  },
  "type": "module",
  "exports": "./src/index.ts",
  "types": "./src/index.ts",
  "files": ["./dist", "./src"],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "prepack": "pnpm run build && clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@rshirohara/kkast": "workspace:^",
    "@rshirohara/pxast": "workspace:^",
    "unified": "^11.0.5",
    "vfile": "^6.0.3"
  },
  "devDependencies": {
    "@rshirohara/repixe-parse": "workspace:*",
    "@rshirohara/repixe-stringify": "workspace:*",
    "@unified-webnovel/tsconfig": "workspace:*",
    "@vivliostyle/vfm": "^2.2.1"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "clean-package": {
    "replace": {
      "exports": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "remove": [
      "scripts.build",
      "devDependencies.@unified-webnovel/tsconfig",
      "publishConfig",
      "clean-package"
    ]
  }
}
